{
  "RENDER_ENVIRONMENT": "wijnemenjemee",
  "BASE_URL": "https://wijnemenjemee.nl/staging/wijnemenjemee/",
  "ASSETS_URL": "https://wijnemenjemee.nl/staging/wijnemenjemee/assets/",
  "REMOTE_HOST": "vra",
  "REMOTE_PATH": "/var/www/vhosts/wijnemenjemee.nl/httpdocs/staging",
  "RENDER_TASKS": ["searchposts"],
  "RENDER_TYPES": ["homepage","post","page","theme","search"],
  "ALGOLIA_INDEX_NAME_PREFIX": "WNJM-ts",
  "ALGOLIA_APP_ID": "RLZJT7BFZT",
  "ALGOLIA_API_KEY": "f0dc506dd42b55db93e55d28c3e667b5",
  "TEMPLATE_DATA": [
    {
      "template" : "post",
      "properties" : [
        {
          "key" : "relatedPosts",
          "query" : [
            { "type": "post" },
            { "catSlugs": { "operator" : "$in", "property" : "catSlugs" }},
            { "slug": { "operator" : "$ne", "property" : "slug" }}
          ],
          "sort": { "date" : -1 },
          "limit": 6
        },
        {
          "key" : "allPosts",
          "query" : [
            { "type": "post" },
            { "slug": { "operator" : "$ne", "property" : "slug" }}
          ],
          "sort": { "date" : -1 },
          "limit": 6
        }
      ]
    },
    {
      "template" : "homepage",
      "properties" : [
        {
          "key" : "posts",
          "query" : [
            { "type": "post" },
            { "featuredItem": { "operator" : "$nin", "value" : [true,1,"1"] }}
          ],
          "sort": { "date" : -1 },
          "limit": 16
        },
        {
          "key" : "sticky",
          "query" : [
            { "type": "post" },
            { "featuredItem": { "operator" : "$in", "value" : [true,1,"1"] }}
          ],
          "sort": { "date" : -1 },
          "limit": 8,
          "post": "prepend_stickies.service"
        },
        {
          "key" : "theme",
          "query" : [
            { "type": "theme" },
            { "_id": { "property" : "featuredTheme" }}
          ],
          "sort": { "date" : -1 },
          "limit": 1
        },
        {
          "key" : "comments",
          "query" : [
            { "type": { "operator" : "$in", "value" : ["post","page"] }},
            { "type": "post" }
          ],
          "sort": { "date" : -1 },
          "limit": 10000,
          "post": "collect_all_comments.service"
        }
      ]
    },
    {
      "template" : "theme",
      "properties" : [
        {
          "key" : "posts",
          "query" : [
            { "type": "post" },
            { "tagIds": { "operator" : "$in", "property" : ["themeFields","tagID"], "parseInt" : true }}
          ],
          "sort": { "date": -1 },
          "limit": 4
        }
      ]
    }
  ],
  "CUSTOM_PATHS": [
    {
      "type": "homepage",
      "path" : "/"
    }
  ],
  "RIPPLES": [
    {
      "types" : ["post","theme"],
      "condition" : false,
      "objects" : [{
        "type": "homepage",
        "slug": "home",
        "path": "/"
      }]
    },
    {
      "types" : ["post"],
      "condition" : false,
      "objects" : [{
        "type": "search",
        "slug": "zoeken",
        "path": "/zoeken"
      }]
    }
  ]
}
